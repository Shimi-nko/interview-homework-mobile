{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "@routes/*": ["routes/*"],
      "@service/*": ["service/*"],
      "@client/*": ["client/*"],
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"]
    }
  }
}
